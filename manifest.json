{
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "homepage_url": "https://github.com/mattes/chrome-ext-tabs-manager",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage", "tabs", "chrome://favicon/"
  ],
  "browser_action": {
    "default_title": "Tabs Manager",      
    "default_popup": "src/popup/index.html",
    "default_icon": {                    // optional
      "19": "icons/browser_action19.png",           // optional
      "38": "icons/browser_action38.png"            // optional
    }
  }
}